{
    "name": "spatie/laravel-responsecache",
    "description": "Greatly speed up a Laravel application by caching the entire response",
    "keywords": [
        "spatie",
        "laravel-responsecache",
        "laravel",
        "cache",
        "response",
        "performance"
    ],
    "homepage": "https://github.com/spatie/laravel-responsecache",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ResponseCache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ResponseCache\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
